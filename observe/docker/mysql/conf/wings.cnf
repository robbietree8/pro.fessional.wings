[mysqld]
authentication_policy       = caching_sha2_password
lower_case_table_names      = 1
innodb_ft_min_token_size    = 2
ft_min_word_len             = 2
ngram_token_size            = 2
character-set-server        = UTF8MB4
default-time-zone           = '+8:00'
log_bin_trust_function_creators = 1
innodb_file_per_table           = 1
#skip_grant_tables